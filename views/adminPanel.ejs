<%- include("partials/header.ejs") %>
<div class="container">  
  <div class="row">
    <% users.forEach(user => { %>
      <div class="col-lg-6">
        <div class="card">
          <div class="card-header bg-dark">
            <%= user.name %> 
          </div>
          <ul class="list-group">
            <% user.listItems.forEach(items => { %>
              <li class="list-group-item">
                  <%= items.item %>
              </li>
            <% }) %>
          </ul>
        </div>
      </div>
    <% }) %>
  </div>
<%- include("partials/footer.ejs") %>